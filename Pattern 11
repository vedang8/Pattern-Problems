#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int n = 5, cnt = 0;
    for(int i=1; i<=n; i++){
        if(i % 2 != 0){
            cnt = 1;
        }
        else{
           cnt = 0;        
        }
        for(int j=1; j<=i; j++){
            cout << cnt << " ";
            (cnt == 1) ? cnt = 0 : cnt = 1;
        }
        cout << endl;
    }
    return 0;
}
