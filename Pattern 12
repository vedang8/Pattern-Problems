#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int n = 5, cnt = 0;
    for(int i=1; i<=n; i++){
        cnt = 0;
        for(int j=1; j<=i; j++){
            cnt++;
            cout << cnt << " ";
        }
        for(int j=1; j<=2*(n-i); j++){
            cout << " " << " ";
        }
        for(int j=1; j<=i; j++){
            cout << cnt << " ";
            cnt--;
        }
        cout << endl;
    }
    return 0;
}
